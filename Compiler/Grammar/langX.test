DECLARE STRING c "Hello";
PRINT c;
ASSIGN c c + " World";
PRINT c;
READ c;
PRINT c;

PRINT "SHORT";
DECLARE SHORT s 5;
PRINT s;
ASSIGN s 10;
PRINT s;
READ s;
PRINT s;
PRINT s + 3;
PRINT s - 3;
PRINT s * 3;
PRINT s / 3;

PRINT "INTEGER";
DECLARE INTEGER f 50;
PRINT f;
ASSIGN f 100;
PRINT f;
READ f;
PRINT f;
PRINT f + 30;
PRINT f - 30;
PRINT f * 3;
PRINT f / 3;

PRINT "LONG";
DECLARE LONG l 500;
PRINT l;
ASSIGN l 1000;
PRINT l;
READ l;
PRINT l;
PRINT l + 300;
PRINT l - 300;
PRINT l * 3;
PRINT l / 3;

PRINT "FLOAT";
DECLARE FLOAT d 5.5;
PRINT d;
ASSIGN d 10.5;
PRINT d;
READ d;
PRINT d;
PRINT d + 5.5;
PRINT d - 5.5;
PRINT d * 5.5;
PRINT d / 5.5;

PRINT "DOUBLE";
DECLARE DOUBLE e 67.5;
ASSIGN e 100.5;
PRINT e;
READ e;
PRINT e;
PRINT e + 10.5;
PRINT e - 10.5;
PRINT e * 10.5;
PRINT e / 10.5;